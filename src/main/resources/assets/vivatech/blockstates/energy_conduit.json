{
  "multipart": [
    {
      "apply": {
        "model": "vivatech:block/energy_conduit_center"
      }
    },
    {
      "when": {
        "AND": [
          {
            "connected_east": "!disconnected"
          },
          {
            "connected_east": "!disabled"
          }
        ]
      },
      "apply": {
        "model": "vivatech:block/energy_conduit_side",
        "x": 270,
        "y": 90
      }
    },
    {
      "when": {
        "AND": [
          {
            "connected_south": "!disconnected"
          },
          {
            "connected_south": "!disabled"
          }
        ]
      },
      "apply": {
        "model": "vivatech:block/energy_conduit_side",
        "x": 90
      }
    },
    {
      "when": {
        "AND": [
          {
            "connected_west": "!disconnected"
          },
          {
            "connected_west": "!disabled"
          }
        ]
      },
      "apply": {
        "model": "vivatech:block/energy_conduit_side",
        "x": 90,
        "y": 90

      }
    },
    {
      "when": {
        "AND": [
          {
            "connected_north": "!disconnected"
          },
          {
            "connected_north": "!disabled"
          }
        ]
      },
      "apply": {
        "model": "vivatech:block/energy_conduit_side",
        "x": 270
      }
    },
    {
      "when": {
        "AND": [
          {
            "connected_up": "!disconnected"
          },
          {
            "connected_up": "!disabled"
          }
        ]
      },
      "apply": {
        "model": "vivatech:block/energy_conduit_side",
        "x": 180
      }
    },
    {
      "when": {
        "AND": [
          {
            "connected_down": "!disconnected"
          },
          {
            "connected_down": "!disabled"
          }
        ]
      },
      "apply": {
        "model": "vivatech:block/energy_conduit_side"
      }
    }
  ]
}